{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/(tabs)/SignUp.jsx",
    "app/(tabs)/Onboarding.jsx",
    "app/(tabs)/Login.jsx",
    "app/(tabs)/Index.jsx",
    "nativewind-env.d.ts"
  ]
}